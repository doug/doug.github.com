<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Doug Fritz | Pile of Data</title>
 <link href="http://www.dougfritz.com/atom.xml" rel="self"/>
 <link href="http://www.dougfritz.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.dougfritz.com/</id>
 <author>
   <name>Doug Fritz</name>
   <email>doug@media.mit.edu</email>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.dougfritz.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.dougfritz.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>